{
  "name": "NS-Document-Management-System",
  "version": "1.0.0",
  "description": "This is the Non Sucking Document Management System which is unlike other document management systems that suck",
  "main": "index.js",
  "scripts": {
    "start:client:dev": "webpack-dev-server --config webpack.config.dev.babel.js --progress --colors",
    "start:api:dev": "nodemon --exec babel-node --presets es2015 ./server/index",
    "start:web:dev": "cross-env npm-run-all --parallel start:client:dev start:api:dev",
    "reset:db": "./node_modules/.bin/sequelize db:migrate:undo:all && sequelize db:migrate",
    "refill:db": "./node_modules/.bin/sequelize db:seed:all",
    "reset:db:test": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "refill:db:test": "NODE_ENV=test sequelize db:seed:all",
    "setup:test-server": "npm run reset:db:test && npm run refill:db:test",
    "server-test": "npm run setup:test-server && NODE_ENV=test nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register ./server/tests/**/*.js --timeout 90000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.prod.babel.js --progress --colors",
    "test": "npm run setup:test-server && npm run server-test",
    "postinstall": "cross-env NODE_ENV=production npm run reset:db && npm run refill:db && npm run build",
    "start": "cross-env NODE_ENV=production node ./lib/server/index",
    "clean": "rimraf lib && mkdir lib",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:server": "babel server -d lib/server --copy-files --ignore test.js"
  },
